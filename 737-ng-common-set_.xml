	<systems>
		<apu>
			<rpm type="float">0</rpm>
			<egt type="float">0</egt>
		</apu>
		<pitot n="1">
			<serviceable type="bool">true</serviceable>
		</pitot>
		<pitot n="2">
			<serviceable type="bool">true</serviceable>
		</pitot>
		<pitot n="3">
			<serviceable type="bool">true</serviceable>
		</pitot>
