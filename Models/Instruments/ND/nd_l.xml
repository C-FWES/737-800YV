<?xml version="1.0"?>

<PropertyList>

<path>nd_l.ac</path>

	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<or>
			<less-than>
				<property>/systems/electric/elec-buses/ac-bus-1/volts</property>
				<value>110</value>
			</less-than>
			<less-than>
				<property>/systems/electric/elec-buses/ac-bus-2/volts</property>
				<value>110</value>
			</less-than>
			<less-than>
				<property>/systems/electric/elec-buses/AC-stby-bus/volts</property>
				<value>110</value>
			</less-than>
			</or>
		</condition>
	</animation>

<animation>
	<type>pick</type>
	<object-name>ndScreenL</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>nd.showNd();</script>
		</binding>
	</action>
</animation>

</PropertyList>

