<?xml version="1.0"?>

<PropertyList>
    <path>efis-control-panel.ac</path>

    <animation>
        <type>knob</type>
        <object-name>Range.knob</object-name>
        <property>instrumentation/efis[0]/inputs/range-knob</property>
        <factor>30</factor>
        <offset-deg>-5</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0128</y-m>
            <z-m>0.0128</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <increase>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(0,"RANGE",1);</script>
            </binding>
        </increase>
        <decrease>
            <binding>
                <command>nasal</command>
                <script>boeing737.efis_ctrl(0,"RANGE",-1);</script>
            </binding>
        </decrease>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>ND.center</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
            <command>property-toggle</command>
            <property>/instrumentation/efis[0]/inputs/nd-centered</property>
        </binding>
        </action>
    </animation>

