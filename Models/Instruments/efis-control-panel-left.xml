    <animation>
        <type>translate</type>
        <object-name>Lvor.toggle</object-name>
        <property>instrumentation/efis/inputs/lh-vor-adf</property>
        <factor>0.003</factor>
        <center>
            <x-m>0.030</x-m>
            <y-m>-0.361</y-m>
            <z-m>-0.014</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>Lvor.vor</object-name>
        <action>
            <name>Left VOR ADF switch to VOR</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/efis/inputs/lh-vor-adf</property>
                <value>1</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>Lvor.off</object-name>
        <action>
            <name>Left VOR ADF switch off</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/efis/inputs/lh-vor-adf</property>
                <value>0</value>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>Lvor.adf</object-name>
        <action>
            <name>Left VOR ADF switch to ADF</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/efis/inputs/lh-vor-adf</property>
                <value>-1</value>
            </binding>
        </action>
    </animation>



    <animation>
        <type>translate</type>
        <object-name>Rvor.toggle</object-name>
        <property>instrumentation/efis/inputs/rh-vor-adf</property>
        <factor>0.003</factor>
        <center>
            <x-m>0.030</x-m>
            <y-m>0.270</y-m>
            <z-m>0.014</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>Rvor.vor</object-name>
        <action>
            <name>Right VOR ADF switch to VOR</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/efis/inputs/rh-vor-adf</property>
                <value>1</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>Rvor.off</object-name>
        <action>
            <name>Right VOR ADF switch off</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/efis/inputs/rh-vor-adf</property>
                <value>0</value>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>Rvor.adf</object-name>
        <action>
            <name>Right VOR ADF switch to ADF</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/efis/inputs/rh-vor-adf</property>
                <value>-1</value>
            </binding>
        </action>
    </animation>

</PropertyList>