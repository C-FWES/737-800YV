    <animation>
        <type>translate</type>
        <object-name>Lvor.toggle</object-name>
        <property>instrumentation/efis/inputs/lh-vor-adf</property>
        <factor>0.003</factor>
        <center>
            <x-m>0.030</x-m>
            <y-m>-0.361</y-m>
            <z-m>-0.014</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
