<?xml version="1.0"?>

<PropertyList>

<path>apu_egt.ac</path>

<animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle</object-name>
  <emission>
    <red>0.5</red>
    <green>0.5</green>
    <blue>0.5</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>systems/apu/egt</property>
  <interpolation>
    <entry><ind>  0</ind><dep>  5</dep></entry>
    <entry><ind>200</ind><dep> 55</dep></entry>
    <entry><ind>400</ind><dep>104</dep></entry>
    <entry><ind>600</ind><dep>148</dep></entry>
    <entry><ind>800</ind><dep>188</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>
