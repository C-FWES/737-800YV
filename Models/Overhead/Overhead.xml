		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep> 30</dep></entry>
		</interpolation>
		<center>
			<x-m>0.07664</x-m>
			<y-m>0</y-m>
			<z-m>0.07776</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
	</animation>
	
	<!-- Engine Gen 1 -->
	<animation>
