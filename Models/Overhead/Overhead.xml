<PropertyList>

	<path>Overhead.ac</path>
	
	<!-- Battery Cover/Switch -->
	<animation>
		<type>pick</type>
		<object-name>batt-cover</object-name>       
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>     
			<binding>
				<command>property-toggle</command>
				<property>controls/OH/batt-cover</property>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>controls/OH/batt-cover</property>
						<value>0</value>
					</equals>
				</condition>
				<command>property-assign</command>
				<property>controls/electric/battery-switch</property>
				<value>1</value>
			</binding>
		</action> 
	</animation>
	
	<animation>
		<type>rotate</type>   
		<object-name>batt-cover</object-name>  
		<property>controls/OH/batt-cover</property>
		<factor>-60</factor>
		<center>
			<x-m>-0.11915</x-m>
			<y-m>0</y-m>
			<z-m>0.08976</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>batt-switch</object-name>       
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>     
			<binding>
				<condition>
					<equals>
						<property>controls/OH/batt-cover</property>
						<value>1</value>
					</equals>
				</condition>
				<command>property-toggle</command>
				<property>controls/electric/battery-switch</property>
			</binding>
		</action> 
	</animation>
	
	<animation>
		<type>rotate</type>   
		<object-name>batt-switch</object-name>  
		<property>controls/electric/battery-switch</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
